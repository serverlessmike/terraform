    // kms encrypted tsa_host_key
    tsa_host_key = ""

    // kms encrypted tsa_host_key.pub
    tsa_host_key_pub = ""

    // kms encrypted worker_key
    worker_key = ""

    // kms encrypted worker_key.pub
    worker_key_pub = ""
  }
}
